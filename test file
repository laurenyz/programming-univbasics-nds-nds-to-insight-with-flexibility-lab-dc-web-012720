def gross_per_studio(collection)
collection = [{movie1 data}, {movie2 data}, ... ]
result = {studio1 => worldwide_gross, studio2 => worldwide_gross, ...}

index = 0
result = {}
  while index < collection.length do 
    studio = collection[index][:studio]
    if result[studio]
      result[studio] 
  end 
 
end